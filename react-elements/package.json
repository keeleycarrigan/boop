    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "lodash": "^4.17.21",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "source-map-loader": "^0.2.4",
    "storybook-readme": "^4.0.5",
    "styled-components": "^4.1.3"
